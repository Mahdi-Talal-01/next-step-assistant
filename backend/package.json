{
  "name": "next-step-assistant-backend",
  "version": "1.0.0",
  "description": "Backend for Next Step Assistant",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs --coverage",
    "db:test:push": "dotenv -e .env.test -- npx prisma db push",
    "db:test:seed": "dotenv -e .env.test -- node prisma/seed.js",
    "db:test:studio": "dotenv -e .env.test -- npx prisma studio"
  },
  "dependencies": {
    "@langchain/openai": "^0.5.10",
    "@prisma/client": "^5.10.2",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.26",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.0.1",
    "node-nlp": "^5.0.0-alpha.5",
    "openai": "^4.98.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prisma": "^5.10.2",
    "supertest": "^7.1.1"
  }
}
