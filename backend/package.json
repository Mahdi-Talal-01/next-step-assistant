{
  "name": "next-step-assistant-backend",
  "version": "1.0.0",
  "description": "Backend for Next Step Assistant",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:test:push": "dotenv -e .env.test -- npx prisma db push",
    "db:test:seed": "dotenv -e .env.test -- node prisma/seed.js",
    "db:test:studio": "dotenv -e .env.test -- npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.98.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prisma": "^5.10.2",
    "supertest": "^7.1.1"
  }
}
